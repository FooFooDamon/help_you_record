    <string name="sql_create_categories_table">
        CREATE TABLE `categories` (
            `category_id`   INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
            `name`  TEXT NOT NULL UNIQUE,
            `remarks`   TEXT
        )
    </string>

    <string name="sql_make_categories_data">
        insert into
        categories(name)
        values(?)
    </string>

    <string name="sql_query_category_names">
        select name
        from categories
        order by category_id asc
    </string>

    <string name="sql_query_unlimited_category_records">
        select category_id, name
        from categories
        order by category_id asc
        limit 1000
    </string>

    <string name="sql_query_category_records_by_name">
        select category_id, name
        from categories
        where name like ?
        order by category_id asc
        limit 1000
    </string>

    <string name="sql_query_category_details_by_id">
        select name, remarks
        from categories
        where category_id = ?
    </string>

    <string name="sql_update_category_details_by_id">
        update `categories`
        set `name` = ?, `remarks` = ?
        where `category_id` = ?
    </string>

    <string name="sql_create_items_table" formatted="false">
        CREATE TABLE `items` (
            `item_id`   INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
            `name`  TEXT NOT NULL UNIQUE,
            `category`  INTEGER NOT NULL,
            `brief` TEXT,
            `details`   TEXT,
            `remarks`   TEXT,
            `createtime`    timestamp DEFAULT(datetime(\'now\',\'localtime\')),
            `updatetime`    timestamp DEFAULT(datetime(\'now\',\'localtime\'))
        )
    </string>

    <string name="sql_make_items_data">
        insert into
        items(name, category, details, createtime, updatetime)
        values(?, ?, ?, datetime(\'now\',\'localtime\'), datetime(\'now\',\'localtime\'))
    </string>

    <string name="sql_query_unlimited_items">
        select item_id, name
        from items
        order by category asc, updatetime desc
        limit 1000
    </string>

